		"inspect-with-kind": "^1.0.3"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^5.0.0",
		"ansi-styles": "^3.2.0",
		"eslint": "^4.12.1",
		"nyc": "^11.4.0",
		"tape": "^4.8.0"
	},
	"eslintConfig": {
		"extends": "@shinnn/node"
