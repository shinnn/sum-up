    "summary",
    "brief",
    "outline",
    "info",
    "information",
    "description",
    "data",
    "package",
    "color",
    "cli-friendly"
  ],
  "dependencies": {
    "chalk": "^1.1.3"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node-legacy": "^3.0.0",
    "eslint": "^3.12.0",
    "istanbul": "^0.4.5",
    "tape": "^4.6.3"
  },
  "eslintConfig": {
    "extends": "@shinnn/node-legacy",
    "rules": {
      "complexity": [
        "error",
        15
      ]
    }
  }
