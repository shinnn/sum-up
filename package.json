{
	"name": "sum-up",
	"version": "3.0.0",
	"description": "Summarize package information",
	"repository": "shinnn/sum-up",
	"homepage": "https://github.com/shinnn/sum-up",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
